                              <TableColumn fx:id="readerBorrowLibrarian" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Bibliotekarz" />
                          </columns>
                        </TableView>
                        <Button mnemonicParsing="false" onAction="#onReturnReaderBorrowButtonPressed" text="Oddaj zaznaczoną książkę">
                           <VBox.margin>
                              <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
                           </VBox.margin>
                        </Button>
                     </children>
