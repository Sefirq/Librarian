                           </VBox.margin>
                        </Button>
                        <Button fx:id="nextPostButton" mnemonicParsing="false" onAction="#onNextPostButtonPressed" text="Następny etat">
                           <VBox.margin>
                              <Insets bottom="16.0" left="16.0" right="16.0" top="8.0" />
                           </VBox.margin>
                        </Button>
                                <Button fx:id="savePostButton" mnemonicParsing="false" onAction="#onSavePostButtonPressed" text="Zapisz">
                                    <VBox.margin>
                                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
                                    </VBox.margin>
                                </Button>
                            </children>
                            <padding>
                                <Insets left="16.0" right="16.0" />
                            </padding>
                        </VBox>
                  <VBox fx:id="librarianLendBox" alignment="CENTER">
                     <children>
                              <TableView fx:id="librarianLends">
                                  <columns>
                                      <TableColumn fx:id="librarianLendTitle" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Tytuł" />
                                      <TableColumn fx:id="librarianLendAuthor" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Autor" />
                                      <TableColumn fx:id="librarianLendReader" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Czytelnik" />
