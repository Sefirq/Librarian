                                      <TableColumn fx:id="librarianLendTill" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Do" />
                                      <TableColumn fx:id="librarianLendLibrary" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Biblioteka" />
                                      <TableColumn fx:id="librarianLendBranch" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Filia" />
                                      <TableColumn fx:id="librarianLendSignature" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="Sygnatura" />
                              <TableColumn fx:id="librarianLendISBN" editable="false" maxWidth="1024" minWidth="100" prefWidth="100" text="ISBN" />
