<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" stylesheets="@materialfx-without-checkbox.css" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ml.adamsprogs.librarian.ReaderPickerController">
   <children>
      <TextField fx:id="searchBar" promptText="Szukaj" />
      <ListView fx:id="readerList" />
      <Button mnemonicParsing="false" onAction="#onChooseReaderButtonPressed" text="Wybierz">
         <VBox.margin>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Anuluj" />
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
